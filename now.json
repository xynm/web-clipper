{
  "name": "web-clipper-resource",
  "version": 2,
  "builds": [
    { "src": "/documents/privacy/*", "use": "@now/static" },
    { "src": "/src/changelog/*", "use": "@now/static" },
    { "src": "/config.json", "use": "@now/static" }
  ],
  "alias": ["resource.clipper.website"],
  "routes": [
    {
      "src": "/privacy/(.*)",
      "dest": "/documents/privacy/$1"
    },
    {
      "src": "/changelog/(.*)",
      "dest": "/src/changelog/$1"
    },
    {
      "src": "/config.json",
      "dest": "/config.json"
    },
    {
      "src": "/.*",
      "status": 404
    }
  ]
}
